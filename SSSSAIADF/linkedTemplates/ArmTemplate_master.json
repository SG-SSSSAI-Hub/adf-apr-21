{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "SSSSAIADF"
		},
		"lsSQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsSQL'"
		},
		"lsStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsStorage'"
		},
		"trigger1_properties_pipeline1_parameters_fileName": {
			"type": "string",
			"defaultValue": "planes.csv"
		},
		"trigger1_properties_pipeline1_parameters_tableName": {
			"type": "string",
			"defaultValue": "Planes"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "SSSSAIADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lsSQL_connectionString": {
						"value": "[parameters('lsSQL_connectionString')]"
					},
					"lsStorage_connectionString": {
						"value": "[parameters('lsStorage_connectionString')]"
					},
					"trigger1_properties_pipeline1_parameters_fileName": {
						"value": "[parameters('trigger1_properties_pipeline1_parameters_fileName')]"
					},
					"trigger1_properties_pipeline1_parameters_tableName": {
						"value": "[parameters('trigger1_properties_pipeline1_parameters_tableName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}